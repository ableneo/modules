{
  "name": "eslint-config-ableneo",
  "version": "1.0.0",
  "description": "Eslint config with prettier @ableneo/modules",
  "main": "src/.eslintrc.js",
  "author": "Marcel Moko≈° <https://github.com/marcelmokos>",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-config with prettier",
    "eslint-config for typescript",
    "eslint-config for flow-type",
    "react-app config",
    "prettier",
    "react",
    "jest",
    "flow",
    "flowtype",
    "typescript",
    "typescript-eslint",
    "format",
    "pretty-print",
    "code-style"
  ],
  "license": "MIT",
  "scripts": {
    "test": "react-scripts test --no-watch",
    "test:changed": "yarn test --onlyChanged --passWithNoTests --silent",
    "test:watch": "react-scripts test",
    "test:update": "yarn test --updateSnapshot",
    "test:coverage": "yarn test:changed --coverage --verbose",
    "eslintrc:check:yml": "eslint --print-config .eslintrc.yml | eslint-config-prettier-check",
    "eslintrc:yml-to-js": "echo '// generated file from .eslintrc.yml\nmodule.exports =' > src/.eslintrc.js && js-yaml .eslintrc.yml >> src/.eslintrc.js",
    "eslintrc:check:js": "eslint --print-config src/.eslintrc.js | eslint-config-prettier-check",
    "eslintrc:prepare": "yarn run eslintrc:check:yml && yarn run eslintrc:yml-to-js && yarn run eslintrc:check:js",
    "pretest:coverage": "yarn run eslintrc:prepare && yarn run test-output:prepare && eslint --print-config .eslintrc.yml > src/output/eslintrc.json",
    "test-output:copy": "cat src/test-markup.js > src/output/test-markup.js && cat src/test-markup-typescipt.tsx > src/output/test-markup-typescipt.tsx",
    "test-output:fix": "eslint --quiet --fix src/output/test-markup.js src/output/test-markup-typescipt.tsx",
    "test-output:prepare": "yarn run test-output:copy && yarn run test-output:fix",
    "build": "yarn run eslintrc:prepare",
    "watch": "yarn run build && echo '!!!Just build not running watch!!!'",
    "prepublishOnly": "yarn run build"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-jest": "^22.3.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "js-yaml": "^3.12.2",
    "prettier": "^1.16.4",
    "react": "^16.8.4",
    "typescript": "3.3.x"
  },
  "peerDependencies": {
    "eslint-config-prettier": "4.x",
    "eslint-plugin-jest": "22.x",
    "eslint-plugin-prettier": "3.x",
    "prettier": "1.x"
  },
  "engines": {
    "node": "^6.14.0 || ^8.10.0 || >=9.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ableneo/modules/tree/master/packages/eslint-config-ableneo"
  },
  "bugs": {
    "url": "https://github.com/ableneo/modules/issues"
  },
  "homepage": "https://github.com/ableneo/modules/tree/master/packages/eslint-config-ableneo#readme"
}
